<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<VBox alignment="CENTER" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Upload a Spreadsheet To Be Analyzed">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Text>
      <HBox alignment="CENTER">
         <children>
            <TextField fx:id="filePath" alignment="CENTER" promptText="No File Chosen" />
            <Button fx:id="browseButton" mnemonicParsing="false" text="Browse" />
         </children>
         <VBox.margin>
            <Insets bottom="64.0" top="64.0" />
         </VBox.margin>
      </HBox>
      <Button fx:id="submitButton" alignment="CENTER" mnemonicParsing="false" text="Use Spreadsheet" />
   </children>
</VBox>
